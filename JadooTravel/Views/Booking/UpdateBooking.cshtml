@model UpdateBookingDto
@{
    ViewData["Title"] = "UpdateBooking";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <br /> <br />
    <form method="post">

        <label>İsim Soyisim Giriniz</label>
        <input type="text" asp-for="FullName" class="form-control" />
        <br />
        <label>Email Giriniz</label>
        <input type="text" asp-for="Email" class="form-control" />
        <br />
        <label>Telefon Giriniz</label>
        <input type="text" asp-for="Phone" class="form-control" />
        <br />
        <label>Mesajınızı Girin</label>
        <input type="text" asp-for="MessageBody" class="form-control" />
        <br />
        <button class="btn btn-primary">Kategori Ekle</button>
    </form>
</div>

