@model CreateFeatureDto
@{
    ViewData["Title"] = "CreateFeature";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="container">
    <br /> <br />
    <form method="post">
        <label>Başlık Giriniz</label>
        <input type="text" asp-for="Title" class="form-control" />
        <br />
        <label>Alt Başlık Giriniz</label>
        <input type="text" asp-for="MainTitle" class="form-control" />
        <br />
        <label>Açıklama Giriniz</label>
        <input type="text" asp-for="Description" class="form-control" />
        <br />
        <label>Video Url Girin</label>
        <input type="text" asp-for="VideoUrl" class="form-control" />
        <br />
        <button class="btn btn-primary">Feature Ekle</button>
    </form>
</div>



